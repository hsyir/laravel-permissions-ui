{
  "name": "hsyir/laravel-permissions-ui",
  "license": "MIT",
  "authors": [
    {
      "name": "Hossein",
      "email": "hosseinyaghmaee@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Hsy\\Permissions\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hsy\\Permissions\\Tests\\": "tests"
    }
  },
  "require": {
    "illuminate/support": "^7 | ^8",
    "spatie/laravel-permission": "4.0"
  },
  "extra": {
    "laravel": {
      "aliases": {
        "Answering": "Hsy\\Permissions\\Facades\\Permissions"
      },
      "providers": [
        "Hsy\\Permissions\\HsyPermissionsServiceProvider"
      ]
    }
  },
  "require-dev": {
    "orchestra/testbench": "6.0",
    "laravel/legacy-factories": "^1.0.4"
  },
  "scripts": {
    "test": "php vendor/phpunit/phpunit/phpunit",
    "test-debug": "php vendor/phpunit/phpunit/phpunit --debug",
    "test-coverage": "php vendor/phpunit/phpunit/phpunit"
  }
}
